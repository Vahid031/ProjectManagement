                            <td data-th="@Html.DisplayNameFor(model => model.ProjectSectionDraft.DraftNumber)">
                                <input type="text" class="form-control txtbox" name="ProjectSectionDraft.DraftNumber">
                            </td>

                            <td data-th="@Html.DisplayNameFor(model => model.ProjectSectionDraft.TempDraftPrice)">
                                <input type="text" class="form-control txtbox" name="ProjectSectionDraft.TempDraftPrice">
