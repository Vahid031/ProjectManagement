                            <th><a onmousedown="Sort('ProjectSectionPrintDraft.DraftDate_', '@(tableName)')">@Html.DisplayNameFor(model => model.ProjectSectionDraft.DraftDate_) <span class="sort-arrow arrow-sortable"></span></a></th>
