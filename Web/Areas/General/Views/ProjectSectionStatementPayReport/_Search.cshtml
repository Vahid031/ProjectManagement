@model ViewModels.ProjectDefine.ProjectSectionStatementPayReportViewModel.ReportParameterProjectSectionStatementPayViewModel
<form id="frm-ReportParameterProjectSectionStatementPay" class="operationform" method="post">
    <div class="form-group col-lg-4 col-md-6">
        @Html.LabelFor(model => model.FileCode, new { @class = "label-txt" })
        @Html.TextBoxFor(model => model.FileCode, new { @class = "form-control txtbox" })
    </div>
    <div class="form-group col-lg-4 col-md-6">
        @Html.LabelFor(model => model.ProjectTitle, new { @class = "label-txt" })
        @Html.TextBoxFor(model => model.ProjectTitle, new { @class = "form-control txtbox" })
    </div>
    <div class="form-group col-lg-4 col-md-6">
        @Html.LabelFor(model => model.ProjectSectionTitle, new { @class = "label-txt" })
        @Html.TextBoxFor(model => model.ProjectSectionTitle, new { @class = "form-control txtbox" })
    </div>
    <div class="form-group col-lg-4 col-md-6">
        @Html.LabelFor(model => model.StatementNumbers, new { @class = "label-txt" })
        @Html.DropDownListFor(model => model.StatementNumberId, new SelectList(Model.StatementNumbers, "Id", "Title"), "انتخاب کنید...", new { @data_val = "false", @class = "form-control txtbox" })
    </div>
    <div class="clear"></div>
    <div class="form-group col-lg-4 col-md-6">
        <button type="submit" id="btnSearchProjectSectionStatementPay" class="btn btn-success"><span class="glyphicon glyphicon-ok" style="float:right;"></span>جستجو</button>
    </div>
</form>
